{
  "name": "ai-sdk-provider-amp-sdk",
  "version": "0.1.0",
  "description": "AI SDK v5 provider for Amp via Amp SDK (@sourcegraph/amp-sdk)",
  "keywords": [
    "ai-sdk",
    "amp-sdk",
    "sourcegraph",
    "amp",
    "language-model",
    "llm"
  ],
  "homepage": "https://github.com/ben-vargas/ai-sdk-provider-amp-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben-vargas/ai-sdk-provider-amp-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ben-vargas/ai-sdk-provider-amp-sdk/issues"
  },
  "license": "MIT",
  "author": "Ben Vargas",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "docs/**/*",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "lint:all": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run typecheck && npm run lint:all && npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "example:basic": "npm run build && npx tsx examples/basic-usage.ts",
    "example:streaming": "npm run build && npx tsx examples/streaming.ts",
    "example:conversation": "npm run build && npx tsx examples/conversation-history.ts",
    "example:config": "npm run build && npx tsx examples/custom-config.ts",
    "example:object": "npm run build && npx tsx examples/generate-object.ts",
    "example:all": "npm run build && npm run example:basic && npm run example:streaming && npm run example:conversation && npm run example:config && npm run example:object"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.9",
    "@sourcegraph/amp-sdk": "^0.1.0-20251022202704-gd85048a"
  },
  "devDependencies": {
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^5.0.47",
    "eslint": "^9.28.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "zod": "^3.0.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
